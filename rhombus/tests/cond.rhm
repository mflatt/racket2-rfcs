#lang rhombus

check:
  cond
  | #true: 17
  | ~else: 18
  ~is 17

check:
  cond
  | #false: 17
  | ~else: 18
  ~is 18

check:
  use_static
  (if #true | [1] | ["one"])[0]
  ~is 1

check:
  ~eval
  use_static
  (if #true | [1] | "one")[0]
  ~throws "specialization not known"

check:
  use_static
  (cond | 1 == 2: [1] | 2 == 2: ["one"])[0]
  ~is "one"

check:
  use_static
  (cond | 1 == 2: [1] | ~else: ["one"])[0]
  ~is "one"

check:
  use_static
  (cond | 1 == 2: [1] | ~else ["one"])[0]
  ~is "one"

check:
  ~eval
  use_static
  (if #true | [1] | "one")[0]
  ~throws "specialization not known"

check:
  ~eval
  use_static
  (cond | #true: [1] | ~else: "one")[0]
  ~throws "specialization not known"
