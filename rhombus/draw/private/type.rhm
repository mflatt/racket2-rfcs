#lang rhombus/static/and_meta

// Rename `XLike.asX` to `XLike` so that argument
// errors are reported as `XLike`, but we get the
// conversions of `XLike.asX`

import:
  "point.rhm":
    rename: PointLike as _PointLike
  .point._PointLike.asPoint as PointLike
  "size.rhm":
    rename: SizeLike as _SizeLike
  .size._SizeLike.asSize as SizeLike
  "rect.rhm":
    rename: RectLike as _RectLike
  .rect._RectLike.asRect as RectLike

export:
  all_from(.point)
  PointLike
  all_from(.size)
  SizeLike
  all_from(.rect)
  RectLike
